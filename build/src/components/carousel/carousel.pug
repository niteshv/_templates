mixin carousel(place)
	section(class="section section-carousel")
		.wrapper(data-carousel, data-flickity='{"autoPlay":"2000","wrapAround": true}')
			if place === 'home'
				each item in data.homeCarousel
					article
						p= item.carouselText
						p= item.carouselText2
			if place === 'about'
				each item in data.aboutCarousel
					article
						p= item.carouselText
						p= item.carouselText2
