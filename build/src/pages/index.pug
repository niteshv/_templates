extends ../pages-template/layout.pug

block headTitle
	title #{home.pageTitle}
	meta(name='description' content=`${home.metaDescription}`)
	meta(property='og:title' content=`${home.openGraphTitle}`)
	meta(property='og:description' content=`${home.openGraphDescription}`)
	meta(property='og:image' content=`${home.openGraphImage}`)

block body
	h1 #{home.pageTitle}
	.col !{home.pageContent}
	.col
		each item in list
			article
				h2= item.header
	img(src="assets/img/placeholder.png" alt="")